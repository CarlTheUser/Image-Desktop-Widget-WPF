<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Image_Desktop_Widget.Styles">

    <Color x:Key="DirtyWhite">#FAFAFA</Color>
    <SolidColorBrush x:Key="DirtyWhiteBrush" Color="{StaticResource DirtyWhite}"/>

    <Color x:Key="DirtyWhiteShadowLight">#EEEEEE</Color>
    <SolidColorBrush x:Key="DirtyWhiteShadowLightBrush" Color="{StaticResource DirtyWhiteShadowLight}"/>

    <Color x:Key="DirtyWhiteShadowDark">#E0E0E0</Color>
    <SolidColorBrush x:Key="DirtyWhiteShadowDarkBrush" Color="{StaticResource DirtyWhiteShadowDark}"/>

    <Color x:Key="DarkGray">#323232</Color>
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGray}"/>

    <Color x:Key="White">#ffffff</Color>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>

    <Color x:Key="Orange">#f0641e</Color>
    <SolidColorBrush x:Key="OrangeBrush" Color="{StaticResource Orange}"/>

    <Color x:Key="Yellow">#ffdd1e</Color>
    <SolidColorBrush x:Key="YellowBrush" Color="{StaticResource Yellow}"/>

    <Color x:Key="Black">#666</Color>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>


</ResourceDictionary>