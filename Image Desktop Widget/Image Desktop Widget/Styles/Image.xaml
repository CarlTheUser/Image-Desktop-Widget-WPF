<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Image_Desktop_Widget.Styles">

    <BitmapImage 
                x:Key="CorkBackground"
                CacheOption="Default"
                UriSource="pack://application:,,,/Images/Background.jpg"/>
    <BitmapImage 
                x:Key="CloseIconSmall"
                CacheOption="OnLoad"
                UriSource="pack://application:,,,/Images/Icons/close-16.png"/>
    <BitmapImage 
                x:Key="HomeIconSmall"
                CacheOption="OnLoad"
                UriSource="pack://application:,,,/Images/Icons/home-16.png"/>
    <BitmapImage 
                x:Key="SettingsIconSmall"
                CacheOption="OnLoad"
                UriSource="pack://application:,,,/Images/Icons/settings-16.png"/>
    <BitmapImage 
                x:Key="DeleteIconSmall"
                CacheOption="OnLoad"
                UriSource="pack://application:,,,/Images/Icons/trash-16.png"/>
    <BitmapImage 
                x:Key="EditIconSmall"
                CacheOption="OnLoad"
                UriSource="pack://application:,,,/Images/Icons/edit-16.png"/>
    

</ResourceDictionary>